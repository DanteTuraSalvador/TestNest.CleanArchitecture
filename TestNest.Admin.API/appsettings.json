{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/testnest-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}",
          "fileSizeLimitBytes": 104857600,
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithProcessId",
      "WithProcessName",
      "WithThreadId",
      "WithCorrelationId"
    ],
    "Properties": {
      "Application": "TestNest.Admin.API"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiProblemDetails": {
    "BaseUrl": "https://your-api.com/probs"
  },
  "Cors": {
    "PolicyName": "TestNestCorsPolicy",
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:4200",
      "http://localhost:5173",
      "https://localhost:3000",
      "https://localhost:4200",
      "https://localhost:5173"
    ],
    "AllowedMethods": [ "GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS" ],
    "AllowedHeaders": [ "Content-Type", "Authorization", "X-Api-Version", "X-Requested-With" ],
    "ExposedHeaders": [ "X-Pagination", "X-Total-Count", "Retry-After" ],
    "AllowCredentials": true,
    "MaxAgeInSeconds": 600
  },
  "KeyVault": {
    "Uri": "https://testnest-kv-2026.vault.azure.net/"
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "",
    "Audience": "",
    "ExpirationInMinutes": 60,
    "RefreshTokenExpirationInDays": 7
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowInSeconds": 60,
    "QueueLimit": 10
  },
  "SecurityHeaders": {
    "EnableHsts": true,
    "HstsMaxAgeInDays": 365,
    "HstsIncludeSubDomains": true,
    "HstsPreload": false,
    "ContentSecurityPolicy": "default-src 'self'",
    "XContentTypeOptions": "nosniff",
    "XFrameOptions": "DENY",
    "ReferrerPolicy": "strict-origin-when-cross-origin",
    "PermissionsPolicy": "accelerometer=(), camera=(), geolocation=(), gyroscope=(), magnetometer=(), microphone=(), payment=(), usb=()"
  },
  "ApplicationInsights": {
    "ConnectionString": "",
    "EnableAdaptiveSampling": true,
    "EnableDependencyTracking": true,
    "EnableRequestTracking": true,
    "EnableExceptionTracking": true,
    "EnablePerformanceCounterCollection": true,
    "CloudRoleName": "TestNest.Admin.API"
  },
  "OpenTelemetry": {
    "Enabled": true,
    "ServiceName": "TestNest.Admin.API",
    "ServiceVersion": "1.0.0",
    "OtlpEndpoint": "",
    "EnableConsoleExporter": false,
    "EnableAzureMonitorExporter": false,
    "AzureMonitorConnectionString": "",
    "Tracing": {
      "EnableAspNetCoreInstrumentation": true,
      "EnableHttpClientInstrumentation": true,
      "EnableSqlClientInstrumentation": true,
      "SamplingRatio": 1.0
    }
  },
  "ResponseCaching": {
    "Enabled": true,
    "MaxBodySizeInBytes": 67108864,
    "UseCaseSensitivePaths": false,
    "DefaultDurationInSeconds": 60,
    "CacheProfiles": [
      {
        "Name": "Default",
        "DurationInSeconds": 60,
        "VaryByHeader": "Accept",
        "NoStore": false
      },
      {
        "Name": "Short",
        "DurationInSeconds": 30,
        "VaryByHeader": "Accept",
        "NoStore": false
      },
      {
        "Name": "Long",
        "DurationInSeconds": 300,
        "VaryByHeader": "Accept",
        "NoStore": false
      },
      {
        "Name": "NoCache",
        "DurationInSeconds": 0,
        "NoStore": true
      }
    ]
  },
  "Compression": {
    "Enabled": true,
    "EnableForHttps": true,
    "Level": "Fastest",
    "MimeTypes": [
      "application/json",
      "application/xml",
      "text/plain",
      "text/json",
      "text/xml",
      "text/html",
      "application/javascript",
      "text/css"
    ]
  },
  "Resilience": {
    "Retry": {
      "Enabled": true,
      "MaxRetryAttempts": 3,
      "BaseDelayInMilliseconds": 200,
      "MaxDelayInMilliseconds": 30000,
      "UseExponentialBackoff": true
    },
    "CircuitBreaker": {
      "Enabled": true,
      "FailureRatio": 0.5,
      "MinimumThroughput": 10,
      "SamplingDurationInSeconds": 30,
      "BreakDurationInSeconds": 30
    },
    "Timeout": {
      "Enabled": true,
      "TimeoutInSeconds": 30
    },
    "Bulkhead": {
      "Enabled": true,
      "MaxParallelization": 100,
      "MaxQueuingActions": 50
    }
  },
  "GracefulShutdown": {
    "Enabled": true,
    "TimeoutInSeconds": 30,
    "WaitForRequestsToComplete": true,
    "DrainTimeInSeconds": 5
  },
  "ConnectionPooling": {
    "EnablePooling": true,
    "MinPoolSize": 5,
    "MaxPoolSize": 100,
    "ConnectionLifetimeInSeconds": 0,
    "ConnectionIdleTimeoutInSeconds": 300,
    "MultipleActiveResultSets": true,
    "ConnectTimeoutInSeconds": 30,
    "CommandTimeoutInSeconds": 30,
    "TrustServerCertificate": false,
    "Encrypt": true
  }
}